{
	"ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "classes": true
  },
  "rules": {
    "new-cap": 0,
    "no-path-concat": 0,
    "no-unused-vars": [
        0,
        null
    ],
    "quotes": [
        1,
        "single"
    ],
    "semi": 2,
    "strict": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}
